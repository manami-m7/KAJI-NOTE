<%= stylesheet_link_tag 'edit_new' %>


<h1 class="task-history-h1">Edit History</h1>
<div class="task-history-edit-form-wrapper">
  <%= form_with model: @task_history, local:true do |f| %>
    <h2><%= @task_history.task.task_name %></h2>
    <div class="form-group">
      <%= f.label :start_time, 'start_time' %>
      <%= f.time_select :start_time %>
    </div>

    <div class="form-group">
      <%= f.label :finish_time, 'finish_time' %>
      <%= f.time_select :finish_time %>
    </div>

    <div><%= f.submit "Update User" %></div>
  <% end %>
</div>